{
  "author": "Jonah Snider <jonah@jonah.pw> (https://jonah.pw)",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "language": "exa",
        "path": "./syntaxes/exa.tmLanguage.json",
        "scopeName": "source.exa"
      }
    ],
    "languages": [
      {
        "aliases": [
          "EXA"
        ],
        "configuration": "./language-configuration.json",
        "extensions": [
          ".exa",
          ".exapunks"
        ],
        "id": "exa"
      }
    ]
  },
  "description": "Syntax highlighting for programming language EXA from the game EXAPUNKS by Zachtronics in Visual Studio Code",
  "displayName": "EXA Syntax Highlighting",
  "engines": {
    "vscode": "^1.25.0"
  },
  "icon": "assets/exapunks.png",
  "keywords": [
    "EXAPUNKS",
    "Video Game",
    "syntax",
    "highlight",
    "Zachtronics"
  ],
  "license": "Apache-2.0",
  "name": "exa-syntax-highlighting",
  "publisher": "pizzafox",
  "repository": {
    "type": "git",
    "url": "https://github.com/pizzafox/exa-syntax-highlighting.git"
  },
  "version": "1.3.0"
