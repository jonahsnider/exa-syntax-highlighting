{
  "author": "Jonah Snider <jonah@jonah.pw> (https://jonah.pw)",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "language": "exa",
        "path": "./syntaxes/exa.tmLanguage.json",
        "scopeName": "source.exa"
      }
    ],
    "languages": [
      {
        "aliases": [
          "EXA"
        ],
        "configuration": "./language-configuration.json",
        "extensions": [
          ".exa",
          ".exapunks"
        ],
        "id": "exa"
      }
    ]
  },
  "description": "Syntax highlighting for programming language EXA from the game EXAPUNKS by Zachtronics in Visual Studio Code",
  "displayName": "EXA Syntax Highlighting",
  "engines": {
    "vscode": "^1.25.0"
  },
  "activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"typescript": "^5.2.2"
	},
  "icon": "assets/exapunks.png",
  "keywords": [
    "EXAPUNKS",
    "Video Game",
    "syntax",
    "highlight",
    "Zachtronics"
  ],
  "license": "Apache-2.0",
  "name": "exa-syntax-highlighting",
  "publisher": "pizzafox",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonahsnider/exa-syntax-highlighting.git"
  },
  "version": "1.3.0"
}
